generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model polls {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  question  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
