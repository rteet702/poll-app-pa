{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "lib": ["es6"],
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "outDir": "./dist"
    }
}
